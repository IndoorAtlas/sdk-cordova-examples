<?xml version='1.0' encoding='utf-8'?>
<widget id="com.indooratlas.cordovaExamples" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CordovaExamples</name>
    <description>
        Examples for IndoorAtlas Cordova Plugin
    </description>
    <author email="contact@indooratlas.com" href="http://indooratlas.com">
        IndoorAtlas
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="www/img/android/mdpi.png" density="mdpi" />
        <icon src="www/img/android/hdpi.png" density="hdpi" />
        <icon src="www/img/android/xhdpi.png" density="xhdpi" />
        <icon src="www/img/android/xxhdpi.png" density="xxhdpi" />
        <icon src="www/img/android/xxxhdpi.png" density="xxxhdpi" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon src="www/img/ios/Icon-60@3x.png" width="180" height="180" />
        <icon src="www/img/ios/Icon-60@2x.png" width="120" height="120" />
        <icon src="www/img/ios/Icon-76.png" width="76" height="76" />
        <icon src="www/img/ios/Icon-76@2x.png" width="152" height="152" />
        <icon src="www/img/ios/Icon-40.png" width="40" height="40" />
        <icon src="www/img/ios/Icon-40@2x.png" width="80" height="80" />
        <icon src="www/img/ios/Icon-40@2x.png" width="120" height="120" />
        <icon src="www/img/ios/Icon-small.png" width="29" height="29" />
        <icon src="www/img/ios/Icon-small@2x.png" width="58" height="58" />
        <icon src="www/img/ios/Icon-small@3x.png" width="87" height="87" />
    </platform>
</widget>
